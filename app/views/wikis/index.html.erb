<h1>Wikis#index</h1>
<p>Find me in app/views/wikis/index.html.erb</p>

<section class="wikis">
  <% @wikis.each do |wiki| %>
    <article>
      <%= link_to(wiki) do %>
        <h3><%= wiki.title %></h3>
      <% end %>
      <div class="fl-r">
        <span><%= time_ago_in_words wiki.created_at %></span>
      </div>
    </article>
  <% end %>
</section>